<%= form_for @candidate do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :github_name %><br />
    <%= f.text_field :github_name %>
  </p>
  <p>
    <%= f.label :linked_in %><br />
    <%= f.text_field :linked_in %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </p>
  <p><%= f.submit %></p>
<% end %>
